{
  "name": "random-chinese-name",
  "version": "1.0.0",
  "description": "A rich random naming library",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/demigodliu/random-chinese-name.git"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs",
    "default": "./dist/index.cjs"
  },
  "scripts": {
    "dev": "webpack --mode development",
    "build": "webpack --mode production",
    "karma:init": "karma init",
    "karma:test": "karma start",
    "execute": "node --loader ts-node/esm --inspect"
  },
  "keywords": [
    "random chinese name"
  ],
  "files": [
    "dist",
    "README.md",
    "README.ZH.md"
  ],
  "author": "demigodliu",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "@types/jasmine": "^4.3.1",
    "@types/node": "^18.15.5",
    "babel-loader": "^9.1.2",
    "esm": "^3.2.25",
    "jasmine-core": "^4.6.0",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "^5.1.0",
    "karma-typescript": "^5.5.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": ">=4.4",
    "webpack": "^5.76.2",
    "webpack-cli": "^5.0.1",
    "webpack-merge": "^5.8.0"
  }
}
